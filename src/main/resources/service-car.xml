<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<bean id="car1" class="com.service.Car">
		<property name="make" value="Toyota" />
		<property name="model" value="Prius" />
		<property name="year" value="2020" />
	</bean>
	<bean id="car2" class="com.service.Car">
		<property name="make" value="huyndai" />
		<property name="model" value="Prius" />
		<property name="year" value="2020" />
	</bean>

	<bean id="employee1" class="com.service.Employee">
		<property name="name" value="Adil" />
		<property name="age" value="30" />
	</bean>

	<bean id="srv1" class="com.service.CarService">
		<property name="name" value="Oil change" />
		<property name="date" value="12/12/2020" />
	</bean>
</beans>
   